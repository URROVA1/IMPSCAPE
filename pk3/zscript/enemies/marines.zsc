class ShotgunMarineEnemy : MarineShotgun{
	//$Title "Shotgun Enemy Marine"
	//$Category "Enemies/Enemy Marines"
	//$Angled
	Default{
		Health 80;
		SeeSound "Marine/Site";
		ActiveSound "Marine/Active";
		Monster;
		+COUNTKILL;
		+NOINFIGHTING;
	}
	States{
		Spawn:
			ALY5 A 4 A_MarineLook;
			ALY5 A 4 A_MarineNoise;
			Loop;
		Idle:
			ALY5 A 4 A_MarineLook;
			ALY5 A 4 A_MarineNoise;
			ALY5 A 4 A_MarineLook;
			ALY5 B 4 A_MarineNoise;
			ALY5 B 4 A_MarineLook;
			ALY5 B 4 A_MarineNoise;
			Loop;
		See:
			ALY5 ABCD 4 A_Chase;
			Loop;
		Missile:
			ALY5 E 12 A_M_CheckAttack;
			ALY5 F 10 Bright A_SPosAttack;
			Goto See;

		Pain:
			ALY5 G 4;
			ALY5 G 4 A_Pain;
			Goto See;
		Death:
			ALY5 H 10;
			ALY5 I 10 A_Scream;
			ALY5 J 10 A_NoBlocking;
			ALY5 KLM 10;
			ALY5 N -1;
			Stop;
		XDeath:
			ALY5 O 5;
			ALY5 P 5 A_XScream;
			ALY5 Q 5 A_NoBlocking;
			ALY5 RSTUV 5;
			ALY5 W -1;
			Stop;
		Raise:
			ALY5 MLKJIH 5;
			Goto See;
	}
}


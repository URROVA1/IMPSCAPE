class ChainsawMarineEnemy : MarineChainsaw{
	//$Title "Chainsaw Enemy Marine"
	//$Category "Enemies/Enemy Marines"
	//$Angled
	Default{
		Health 80;
		Speed 16;
		Monster;
		+COUNTKILL;
		+NOINFIGHTING;
	}
	States{
		Spawn:
			ALY7 A 4 {A_MarineLook(); A_StopSound(CHAN_WEAPON);} //Calla el canal CHAN_WEAPON para que no suene la motosierra si esta vigilando.
			ALY7 A 4;
			Loop;
		Idle:
			ALY7 A 4 A_MarineLook;
			ALY7 A 4;
			ALY7 A 4 A_MarineLook;
			ALY7 B 4;
			ALY7 B 4 A_MarineLook;
			ALY7 B 4;
			Loop;
		See:
			//Perdon dios de las buenas practicas de la programacion por hacer esto
			ALY7 A 2 { A_Chase("_a_chase_default", "_a_chase_default", CHF_FASTCHASE); A_MarineNoise(); }
			ALY7 ABB 2 A_Chase("_a_chase_default", "_a_chase_default", CHF_FASTCHASE);
			ALY7 A 2 { A_Chase("_a_chase_default", "_a_chase_default", CHF_FASTCHASE); A_MarineNoise(); }
			ALY7 ABB 2 A_Chase("_a_chase_default", "_a_chase_default", CHF_FASTCHASE);
			Loop;
		Meele:
			ALY7 E 4 A_MarineNoise;
			ALY7 E 4 A_M_Saw;
			ALY7 E 0 A_M_SawRefire;
			Goto Melee+1;
			ALY7 A 0;
			Goto See;
		Pain:
			ALY7 G 4;
			ALY7 G 4 A_Pain;
			Goto See;
		Death:
			ALY7 H 10;
			ALY7 I 10 A_Scream;
			ALY7 J 10 A_NoBlocking;
			ALY7 KLM 10;
			ALY7 N -1;
			Stop;
		XDeath:
			ALY7 O 5;
			ALY7 P 5 A_XScream;
			ALY7 Q 5 A_NoBlocking;
			ALY7 RSTUV 5;
			ALY7 W -1;
			Stop;
		Raise:
			ALY7 MLKJIH 5;
			Goto See;
	}
}

